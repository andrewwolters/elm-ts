{
  "name": "elm-ts",
  "version": "0.0.1",
  "description": "A porting to TypeScript using fp-ts and React",
  "files": [
    "lib",
    "lib-jsnext"
  ],
  "main": "lib/index.js",
  "jsnext:main": "lib-jsnext/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "lint": "tslint src/**/*.ts test/**/*.ts",
    "mocha": "mocha -r ts-node/register test/*.ts",
    "test": "npm run lint && npm run mocha",
    "build": "rm -rf lib/* && tsc && tsc -m es6 --outDir lib-jsnext"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcanti/elm-ts.git"
  },
  "author": "Giulio Canti <giulio.canti@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanti/elm-ts/issues"
  },
  "homepage": "https://github.com/gcanti/elm-ts",
  "dependencies": {
    "fp-ts": "^0.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "rxjs": "^5.2.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.4",
    "@types/react": "^15.0.5",
    "@types/react-dom": "^0.14.22",
    "awesome-typescript-loader": "^3.1.2",
    "mocha": "^3.2.0",
    "ts-node": "^2.0.0",
    "tslint": "^4.4.2",
    "tslint-config-standard": "^4.0.0",
    "typescript": "^2.2.0",
    "webpack": "^1.14.0"
  },
  "tags": [
    "typescript",
    "elm",
    "fp-ts"
  ],
  "keywords": [
    "typescript",
    "elm",
    "fp-ts"
  ]
}
